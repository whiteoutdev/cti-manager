{
  "name": "cti-manager-service",
  "version": "2.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "serve": "node index | bunyan",
    "db": "yarn build && node lib/scripts/rebuildDatabase | bunyan",
    "db-dev": "yarn build && node lib/scripts/rebuildDatabase --dev | bunyan",
    "dev": "yarn db-dev && nodemon -V -e js,ts,scss,html,ejs,json --ignore lib --ignore secrets.json --ignore tmp --exec \"yarn build && node index --dev | bunyan\"",
    "prod": "yarn build && yarn serve",
    "lint": "tslint --fix -c tslint.json -t stylish src/**/*.ts",
    "depcheck": "babel-node build/depcheck",
    "test": "concurrently \"yarn lint\" \"yarn depcheck\"",
    "start": "forever start index.js",
    "stop": "forever stop index.js",
    "restart": "forever restart index.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.15.2",
    "bson": "^1.0.4",
    "bunyan": "^1.8.1",
    "cookie-parser": "^1.4.3",
    "cors": "^2.7.1",
    "del": "^2.2.2",
    "express": "^4.14.0",
    "fluent-ffmpeg": "^2.1.0",
    "forever": "^0.15.3",
    "jimp": "^0.2.28",
    "jsonwebtoken": "^7.4.1",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "mongodb": "^2.2.28",
    "morgan": "^1.7.0",
    "multer": "^1.1.0",
    "passport": "^0.3.2",
    "passport-jwt": "^2.2.1",
    "yargs": "^8.0.1"
  },
  "devDependencies": {
    "@types/bcrypt-nodejs": "^0.0.30",
    "@types/body-parser": "^1.16.3",
    "@types/bunyan": "^0.0.36",
    "@types/cookie-parser": "^1.3.30",
    "@types/cors": "^2.8.1",
    "@types/del": "^2.2.32",
    "@types/express": "^4.0.35",
    "@types/fluent-ffmpeg": "^2.1.1",
    "@types/jsonwebtoken": "^7.2.1",
    "@types/lodash": "^4.14.65",
    "@types/moment": "^2.13.0",
    "@types/mongodb": "^2.2.2",
    "@types/morgan": "^1.7.32",
    "@types/multer": "^0.0.34",
    "@types/passport": "^0.3.3",
    "@types/passport-strategy": "^0.2.30",
    "@types/yargs": "^6.6.0",
    "babel-cli": "^6.24.1",
    "babel-polyfill": "^6.9.1",
    "concurrently": "^3.5.0",
    "depcheck": "^0.6.7",
    "nodemon": "^1.11.0",
    "tslint": "^5.5.0",
    "typescript": "^2.4.2"
  }
}
