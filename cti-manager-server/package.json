    "express": "^4.14.0",
    "lodash": "^4.14.1",
    "lwip": "0.0.9",
    "mongodb": "^2.2.4",
    "multer": "^1.1.0",
    "stream-to-string": "^1.1.0",
    "yargs": "^4.8.1"
  },
  "devDependencies": {
